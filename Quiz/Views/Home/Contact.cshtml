@{
    ViewData["Title"] = "Contact";
}

<section class="section section-lg bg-white">
    <!-- section wave-->
    <div class="section-wave">
        <svg x="0px" y="0px" width="1920px" height="45px" preserveAspectRatio="none" viewBox="0 0 1920 45">
            <path d="M1920,0c-82.8,0-108.8,44.4-192,44.4c-78.8,0-116.5-43.7-192-43.7 c-77.1,0-115.9,44.4-192,44.4c-78.2,0-114.6-44.4-192-44.4c-78.4,0-115.3,44.4-192,44.4C883.1,45,841,0.6,768,0.6 C691,0.6,652.8,45,576,45C502.4,45,461.9,0.6,385,0.6C306.5,0.6,267.9,45,191,45C115.1,45,78,0.6,0,0.6V45h1920V0z"></path>
        </svg>
    </div>
    <div class="shell shell-bigger">
        <div class="range range-ten range-50 range-sm-center range-lg-justify">
            <div class="cell-sm-9 cell-md-5 cell-lg-4">
                <h3>Contact us</h3>
                <hr class="divider divider-left divider-default">
                <img src="~/images/Nerds.png" />
            </div>
            <div class="cell-sm-9 cell-md-4 cell-lg-3">
                <div class="column-aside">
                    <div class="range">
                        <div class="cell-xs-10 cell-sm-6 cell-md-12">
                            <h6>Address</h6>
                            <hr class="divider-thin">
                            <article class="box-inline"><span class="icon icon-md-smaller icon-primary mdi mdi-map-marker"></span><span><a href="#">568 Filadélfia Avenue, West St. <br class="veil reveal-lg-block"> Araguaína - TO, 77816-540</a></span></article>
                        </div>
                        <div class="cell-xs-10 cell-sm-6 cell-md-12">
                            <h6>Phone</h6>
                            <hr class="divider-thin">
                            <article class="box-inline">
                                <span class="icon icon-md-smaller icon-primary mdi mdi-phone"></span>
                                <ul class="list-comma">
                                    <li><a href="callto:#">(63) 3411-8500</a></li>
                                </ul>
                            </article>
                        </div>
                        <div class="cell-xs-10 cell-sm-6 cell-md-12">
                            <h6>E-mail </h6>
                            <hr class="divider-thin">
                            <article class="box-inline"><span class="icon icon-md-smaller icon-primary mdi mdi-email-open"></span><span><a href="/cdn-cgi/l/email-protection#b192"><span class="__cf_email__" data-cfemail="d1bcb0b8bd91b5b4bcbebdb8bfbaffbea3b6">nerds@itpac.br</span></a></span></article>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<script>
    $("#pagina-ativa li").removeClass("active");
    $("#pagina-ativa li").eq(2).addClass("active");
</script>
